<body>
  <div class="bg" style="min-height:500px;" id="hero-top">
    <div class="hero-content app-hero-content text-center" style="padding-top:50px;">
      <div class="container-fluid">
        <div class="row justify-content-md-center">
          <div class="col-md-10">
            <h1 class="wow fadeInUp" data-wow-delay="0s" style="color:#222;">Online<br />Appointment<br />Scheduling</h1>
            <p class="wow fadeInUp" data-wow-delay="0.2s" style="color:#444;">
              Make the most of your time. <br class="hidden-xs"> Try for free and invite clients to<br /> schedule an appointment today.
            </p>
            <a asp-controller="Account" asp-action="TryFree" class="btn btn-primary" style="font-size:2rem;" data-wow-delay="0.2s">Try Free</a>
            <div class="mt-1 text-muted">No credit card required</div>
            <a asp-controller="Account" asp-action="SignIn" class="btn btn-secondary mt-5 btn-lg" data-wow-delay="0.2s">Sign-In</a>
            <br />
            <a asp-controller="Account" asp-action="RegisterAsClientOnly" class="btn  mt-5" data-wow-delay="0.2s">Register as client</a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="wrapper animsition" data-animsition-in-class="fade-in" data-animsition-in-duration="1000" data-animsition-out-class="fade-out" data-animsition-out-duration="1000">
    <div class="container">
      <nav class="navbar navbar-expand-lg navbar-light navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
          <a class="navbar-brand page-scroll" href="#main"><img class="lazyload" data-src="https://cdnappts3.azureedge.net/files/appts/imgs/appts-brand-logo-A-40x37-v3.png" alt="Appts Online" /></a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="feather icon-menu"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto"></ul>
            <ul class="navbar-nav my-2 my-lg-0">
              <li class="nav-item">
                <a class="nav-link page-scroll" href="#how-it-works">How it works</a>
              </li>
              <li class="nav-item">
                <a class="nav-link page-scroll" href="#features">Features</a>
              </li>
              <li class="nav-item">
                <a class="nav-link page-scroll" href="#pricing">Pricing</a>
              </li>
              <li class="nav-item">
                <a class="nav-link page-scroll" href="#solutions">Solutions</a>
              </li>
              <li class="nav-item">
                <a asp-controller="Account" asp-action="SignIn" class="nav-link page-scroll"><i class="feather icon-log-in"></i> Sign-In</a>
              </li>
              <li class="nav-item">
                <a asp-controller="Account" asp-action="TryFree" class="nav-link page-scroll" style="font-size:1.1rem;"><b><i class="feather icon-power"></i> Try Free</b></a>
              </li>
            </ul>
            <br />
          </div>
        </div>
      </nav>
    </div>
    <div class="main" id="main">
      @RenderBody()
      <div class="footer">
        <div class="container">
          <div class="col-md-12 text-center">
            <img class="lazyload" data-src="https://cdnappts3.azureedge.net/files/appts/imgs/appts-brand-logo-A-40x37-v3.png" alt="Appts.Online" />
            <div class="footer-text">
              <p>
                &copy; 2020 - Appts Online
              </p>
            </div>
            <ul class="footer-menu">
              <li><a asp-controller="Help" asp-action="Index">Help</a></li>
              <li><a asp-controller="Legal" asp-action="Privacy">Privacy</a></li>
              <li><a asp-controller="Legal" asp-action="Terms">Terms</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
<script src="https://cdnappts3.azureedge.net/files/appts/js/lazyload.min.js"></script>
  <script>
    lazyload();
  </script>
  <script type="text/javascript" src="~/landing_assets/js/jquery-2.1.1.js"></script>
  <script type="text/javascript" src="~/landing_assets/js/bootstrap.min.js"></script>
  <script type="text/javascript" src="~/landing_assets/js/plugins.js"></script>
  <script type="text/javascript" src="~/landing_assets/js/menu.js"></script>
  <script type="text/javascript" src="~/landing_assets/js/custom.js"></script>
</body>
</html>