@using Appts.Models.Document 
@using Appts.Web.Ui.Scheduler.ViewModels
@model ViewVanityUrlLookupsVm
@{ 
  ViewData["Title"] = "data by user";
}
<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>data by user</h1>
      <h4>Vanity Url Lookups</h4>
      @if (Model.LookupDocuments != null && Model.LookupDocuments.Count > 0)
      {
        int count = 1;
        foreach (var lookup in Model.LookupDocuments)
        {
          <div class="card mb-1" id="@lookup.LookupModel.Value">
            <div class="card-body">
              <h5 class="card-title"><span class="badge badge-dark">@count</span> <a href="javascript:void" class="a-get-docs-for-lookup">@lookup.LookupModel.Key</a></h5>
              <h6 class="card-subtitle mb-2 text-muted">@lookup.LookupModel.Value</h6>
              <pre><code>@lookup.LookupDocumentJson</code></pre>
            </div>
          </div>
          count++;
        }
      }
      <ul class="list-group list-group-flush">
        <li class="list-group-item">
        </li>
      </ul>
    </div>
  </div>
</div>
<div class="modal fade" id="detailModal" tabindex="-1" role="dialog">
  <div class="modal-dialog modal-xl" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="detailModalTitle"></h5>
        <button type="button" class="close" data-dismiss="modal">
          <span>&times;</span>
        </button>
      </div>
      <div class="modal-body p-0">
        <div class="card mt-1" id="user-docs">
          <div class="card-body">
            <h5 class="card-title">user</h5>
            <pre><code id="service-provider-json"></code></pre>
            <pre><code id="client-json"></code></pre>
          </div>
        </div>
        <div class="card mt-1" id="subscription-docs">
          <div class="card-body">
            <h5 class="card-title">subscription</h5>
            <pre><code id="subscription-json"></code></pre>
            <pre><code id="trial-promo-json"></code></pre>
            <pre><code id="stripe-customer-json"></code></pre>
          </div>
        </div>
        <div class="card mt-1" id="availability-docs">
          <div class="card-body">
            <h5 class="card-title">availability</h5>
            <ul class="list-group list-group-flush">
              @*generated by js: <li class="list-group-item"></li>*@
            </ul>
          </div>
        </div>
        <div class="card mt-1" id="appt-type-docs">
          <div class="card-body">
            <h5 class="card-title">appointment types</h5>
            <ul class="list-group list-group-flush">
              @*generated by js: <li class="list-group-item"></li>*@
            </ul>
          </div>
        </div>
        <div class="card mt-1" id="appointment-docs">
          <div class="card-body">
            <h5 class="card-title">appointments</h5>
            <ul class="list-group list-group-flush">
              @*generated by js: <li class="list-group-item"></li>*@
            </ul>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-info" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
@section Scripts
{
  <script src="~/js/appts_a_view_user_lookups.js"></script>
} 